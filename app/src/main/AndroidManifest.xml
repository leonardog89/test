<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.leo.test" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/brinks"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="com.leo.test.SplashScreenActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.leo.test.Presentation.Category.Implementation.CategoriesActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.leo.test.Presentation.AplicationList.Implementation.AplicacionesActivity"
            android:label="@string/title_activity_aplicaciones" >
        </activity>
        <activity
            android:name="com.leo.test.Presentation.AplicationDetail.Implementation.AplicationActivity"
            android:label="@string/title_activity_resumen" >
        </activity>
    </application>

</manifest>
